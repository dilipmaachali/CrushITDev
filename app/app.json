{
  "expo": {
    "name": "CrushIT",
    "slug": "crushit",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "entryPoint": "./index.js",
    "orientation": "portrait",
    "runtimeVersion": "54.0.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.crushit.app"
    },
    "android": {
      "package": "com.crushit.app",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#5A00FF"
      },
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "de3683f5-cf6c-4169-83dd-9c91d3dc070f"
      }
    }
  }
}
